AJ,WAAA,EAAsB;AACpB,MAAA,IAAI,CAAJ,EAAA,CAAA,UAAA,GAAsB,MAAO,KAAA,QAAA,GAA7B,IAAA;;AACA,MAAA,IAAI,CAAJ,EAAA,CAAA,UAAA,GAAsB,MAAO,KAAA,QAAA,GAA7B,KAAA;AACD;;AAED,QAAI,KAAJ,QAAA,EAAmB;AACjB,UAAI,QAAQ,GAAZ,CAAA;AACA,MAAA,QAAQ,GAAG,CAAC,KAAA,MAAA,CAAA,OAAA,IAAD,EAAA,EAAA,GAAA,CAAgC,CAAA,CAAA,EAAA,CAAA,KAAS;AAClD,YAAI,CAAC,CAAD,GAAA,IAAS,OAAO,CAAC,CAAR,gBAAA,KAAT,WAAA,KAAuD,CAAC,CAAD,gBAAA,CAAA,IAAA,CAAA,OAAA,CAAA,IAAA,KAAA,OAAA,IAAoD,CAAC,CAAD,gBAAA,CAAA,IAAA,CAAA,OAAA,CAAA,IAAA,KAA/G,WAAI,CAAJ,EAAsK;AACpK,UAAA,QAAQ;AACR,iBAAO,CAAC,CAAA,KAAA,EAAQ;AACd,YAAA,KAAK,EAAE;AACL,cAAA,eAAe,EAAE,QAAQ,GAAR,IAAA,GAAkB;AAD9B,aADO;AAId,YAAA,GAAG,EAAE;AAJS,WAAR,EAKL,CALH,CAKG,CALK,CAAR;AAFF,SAAA,MAQO;AACL,UAAA,CAAC,CAAD,GAAA,GAAA,CAAA;AACA,iBAAA,CAAA;AACD;AAZH,OAAW,CAAX;AAcD;;AAED,UAAM,IAAI,GAAG,CAAC,CAAA,kBAAA,EAAqB;AACjC,MAAA,KAAK,EAD4B,oBAAA;AAEjC,MAAA,KAAK,EAAE;AACL,QAAA,IAAI,EAAE,KADD,UAAA;AAEL,QAAA,IAAI,EAAE,KAFD,IAAA;AAGL,QAAA,MAAM,EAAE,KAHH,MAAA;AAIL,QAAA,GAAG,EAAE;AAJA;AAF0B,KAArB,EAAd,QAAc,CAAd;AAUA,WAAO,CAAC,CAAA,KAAA,EAAA,IAAA,EAAc,CAAC,KAAA,MAAA,CAAD,SAAA,EAAtB,IAAsB,CAAd,CAAR;AACD;;AAnFoE,CAAxD,CAAf","sourcesContent":["// Styles\nimport './VSpeedDial.sass'\n\n// Mixins\nimport Toggleable from '../../mixins/toggleable'\nimport Positionable from '../../mixins/positionable'\nimport Transitionable from '../../mixins/transitionable'\n\n// Directives\nimport ClickOutside from '../../directives/click-outside'\n\n// Types\nimport mixins from '../../util/mixins'\nimport { VNode, VNodeData } from 'vue'\nimport { Prop } from 'vue/types/options'\n\n/* @vue/component */\nexport default mixins(Positionable, Toggleable, Transitionable).extend({\n  name: 'v-speed-dial',\n\n  directives: { ClickOutside },\n\n  props: {\n    direction: {\n      type: String as Prop<'top' | 'right' | 'bottom' | 'left'>,\n      default: 'top',\n      validator: (val: string) => {\n        return ['top', 'right', 'bottom', 'left'].includes(val)\n      },\n    },\n    openOnHover: Boolean,\n    transition: {\n      type: String,\n      default: 'scale-transition',\n    },\n  },\n\n  computed: {\n    classes (): object {\n      return {\n        'v-speed-dial': true,\n        'v-speed-dial--top': this.top,\n        'v-speed-dial--right': this.right,\n        'v-speed-dial--bottom': this.bottom,\n        'v-speed-dial--left': this.left,\n        'v-speed-dial--absolute': this.absolute,\n        'v-speed-dial--fixed': this.fixed,\n        [`v-speed-dial--direction-${this.direction}`]: true,\n        'v-speed-dial--is-active': this.isActive,\n      }\n    },\n  },\n\n  render (h): VNode {\n    let children: VNode[] = []\n    const data: VNodeData = {\n      class: this.classes,\n      directives: [{\n        name: 'click-outside',\n        value: () => (this.isActive = false),\n      }],\n      on: {\n        click: () => (this.isActive = !this.isActive),\n      },\n    }\n\n    if (this.openOnHover) {\n      data.on!.mouseenter = () => (this.isActive = true)\n      data.on!.mouseleave = () => (this.isActive = false)\n    }\n\n    if (this.isActive) {\n      let btnCount = 0\n      children = (this.$slots.default || []).map((b, i) => {\n        if (b.tag && typeof b.componentOptions !== 'undefined' && (b.componentOptions.Ctor.options.name === 'v-btn' || b.componentOptions.Ctor.options.name === 'v-tooltip')) {\n          btnCount++\n          return h('div', {\n            style: {\n              transitionDelay: btnCount * 0.05 + 's',\n            },\n            key: i,\n          }, [b])\n        } else {\n          b.key = i\n          return b\n        }\n      })\n    }\n\n    const list = h('transition-group', {\n      class: 'v-speed-dial__list',\n      props: {\n        name: this.transition,\n        mode: this.mode,\n        origin: this.origin,\n        tag: 'div',\n      },\n    }, children)\n\n    return h('div', data, [this.$slots.activator, list])\n  },\n})\n"],"sourceRoot":""}]}